{

    "description": "Displays Night Time Data on SLT usage meter. Created by Thambaru Wijesekara",
    "manifest_version": 2,
    "name": "Night Time Data on SLT Usage Meter",
    "version": "0.0.3",
    "homepage_url": "https://thambaru.com",
    "icons": {
        "16": "icons/icon16.png",
        "19": "icons/icon19.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },

    "content_scripts": [
        {
            "matches": ["*://internetvas.slt.lk/*", "*://www.internetvas.slt.lk/*"],
            "js": ["js/jquery/jquery.min.js", "src/inject/inject.js"],
            "css": ["src/inject/inject.css"]
        }
    ]
}